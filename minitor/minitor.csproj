<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>netcoreapp2.0;net46</TargetFrameworks>
    <RootNamespace>Minitor</RootNamespace>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Deterministic>false</Deterministic>
    <ServerGarbageCollection>true</ServerGarbageCollection>
    <ConcurrentGarbageCollection>true</ConcurrentGarbageCollection>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\status.html" />
    <None Remove="Resources\status.js" />
    <None Remove="Resources\usage.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\status.html" />
    <EmbeddedResource Include="Resources\status.js" />
    <EmbeddedResource Include="Resources\usage.txt" />
  </ItemGroup>

</Project>
